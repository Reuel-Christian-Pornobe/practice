   "outputs": [],
   "source": [
    "def fib(n):\n",
    "    # Base case\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    if n == 1:\n",
    "        return 1\n",
    "        \n",
    "    # Recursive case\n",
    "    return fib(n-1) + fib(n-2)"
   ]
  },
